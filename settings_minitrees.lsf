#!/bin/sh

echo $WORKDIRECTORY
echo $SAMPLES
echo $SYSTEMATIC

cd $WORKDIRECTORY/

eval `scramv1 runtime -sh`

stat ./runAnalysisMiniTrees

`cat $SAMPLES | grep root | grep -v "#" | awk -v line=$LSB_JOBINDEX -v syst=$SYSTEMATIC '{ if (NR==line) print "./runAnalysisMiniTrees "$1,syst,$2,$3,$4 }'`

rm core.*